<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="LoggerConfig"
		   targetNamespace="LogConfig"
		   xmlns:xs="http://www.w3.org/2001/XMLSchema"
>
	<xs:element name="Logger">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="FileNameRoot" type="xs:string"/>
				<xs:element name="MaxEventCount" type="xs:unsignedInt" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
